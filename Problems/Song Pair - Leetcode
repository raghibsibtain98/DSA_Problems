class Solution {
    public int numPairsDivisibleBy60(int[] time) {
        int[] counts = new int[60];
        int result = 0;
        for(int i = 0; i<time.length; i++){
            counts[time[i]%60]++ ;
        }
        
        for(int i = 1; i<30; i++){
            result += counts[i] * counts[60-i];
        }
        
        result += (counts[0] * (counts[0]-1))/2 + (counts[30] * (counts[30]-1))/2 ;
        return result;
    }
}
